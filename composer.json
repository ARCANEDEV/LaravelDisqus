{
    "name":        "arcanedev/laravel-disqus",
    "description": "A simple Disqus integration for Laravel 5",
    "keywords":    ["arcanedev", "laravel", "disqus", "comments", "messages", "blog", "post"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelDisqus",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.0",
        "arcanedev/support": "~4.2.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0",
        "phpunit/phpcov":      "~4.0",
        "phpunit/phpunit":     "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelDisqus\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelDisqus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanedev\\LaravelDisqus\\LaravelDisqusServiceProvider"
            ]
        }
    }
}
