{
    "name":        "arcanedev/laravel-disqus",
    "description": "A simple Disqus integration for Laravel 5",
    "keywords":    ["arcanedev", "laravel", "disqus", "comments", "messages", "blog", "post"],
    "homepage":    "https://github.com/ARCANEDEV/LaravelDisqus",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.6.4",
        "arcanedev/support": "~3.20"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LaravelDisqus\\": "src/"
        },
        "files": ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LaravelDisqus\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
